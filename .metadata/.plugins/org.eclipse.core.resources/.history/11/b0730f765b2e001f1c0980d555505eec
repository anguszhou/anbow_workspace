<hibernate-mapping package="com.ambow.ects.entity">

    <class name="Order" table="myorder">
        <id name="id"  column="order_id">
       				<generator class="native"/> 
        </id>
        <property name="name" column="name"/>
        <property name="cost" column="cost"/>
    
    	<set name="orderlines" cascade="all" inverse="true">
   				<key column="order_id"></key>
   				<one-to-many class="OrderLine"/>
   		</set>
   		<many-to-one name="payWay" class="PayWay" column="payWay_id"  cascade="all"></many-to-one>
   		
   		<many-to-one name="status" class="OrderStatus" column="orderStatus_id"  cascade="all"></many-to-one>
   		
   		<many-to-one name="user" class="User" column="user_id"  cascade="all"></many-to-one>
    </class>

</hibernate-mapping>